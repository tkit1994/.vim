{
	"clangd.arguments": [
		"--log=verbose",
		"--pretty",
		"--background-index",
		"--completion-style=detailed",
		"--cross-file-rename",
		"--header-insertion=never",
		"--inlay-hints=true"
	],
	"diagnostic.checkCurrentLine": true,
	"rust-analyzer.server.path": "/usr/bin/rust-analyzer",
	"rust-analyzer.checkOnSave.enable": false,
	"go.goplsPath": "/usr/bin/gopls"
}
